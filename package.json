{
  "name": "next-layout",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tsm": "typed-scss-modules src/**/*.module.scss -n none -e default -d @import'./src/shared/styles/index.scss'; -u",
    "dev": "concurrently \"yarn tsm -w\" \"next dev\"",
    "build": "yarn tsm && next build",
    "start": "concurrently \"yarn tsm -w\" \"next start\"",
    "prepare": "husky install",
    "lint": "next lint"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "ethers": "^5.7.1",
    "mobx": "^6.6.2",
    "mobx-react-lite": "^3.4.0",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.4.0",
    "react-qrcode-logo": "^2.8.0",
    "the-new-css-reset": "^1.7.3",
    "wagmi": "^0.6.8"
  },
  "devDependencies": {
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "concurrently": "^7.3.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "prettier": "2.7.1",
    "sass": "^1.54.8",
    "stylelint": "^14.13.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "typed-scss-modules": "^6.5.0",
    "typescript": "4.8.2"
  }
}
